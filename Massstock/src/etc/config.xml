<?xml version="1.0"?>
<config>
    <modules>
        <Barcala_Massstock>
            <version>1.0.0</version>
        </Barcala_Massstock>
    </modules>

    <global>
        <models>
            <barcala_massstock>
                <class>Barcala_Massstock_Model</class>
            </barcala_massstock>
        </models>
    </global>

    <frontend>
        <routers>
            <barcala_massstock>
              <use>standard</use>
              <args>
                 <module>Barcala_Massstock</module>
                <frontName>massstock</frontName>
              </args>
            </barcala_massstock>
        </routers>
    </frontend>
</config>
